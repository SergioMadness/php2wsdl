<?xml version="1.0"?>
<definitions xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:tns="http://www.www.
ru/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:xsd="http://www.w3
.org/2001/XMLSchema" xmlns:soap-enc="http://schemas.xmlsoap.org/soap/encoding/"
             xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" name="testwsdl" targetNamespace="h
ttp://www.www.ru/">
    <types>
        <xsd:schema targetNamespace="http://www.www.ru/">
            <xsd:import namespace="http://schemas.xmlsoap.org/soap/encoding/"/>
        </xsd:schema>
    </types>
    <portType name="testItPort">
        <operation name="testMethod">
            <input message="tns:testMethodIn"/>
        </operation>
        <operation name="secondMethod">
            <input message="tns:secondMethodIn"/>
        </operation>
    </portType>
    <binding name="testItBinding" type="tns:testItPort">
        <soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>

        <operation name="testMethod">
            <soap:operation soapAction="http://www.www.ru/#testMethod"/>
            <input>
                <soap:body use="encoded" encodingStyle="http://schemas.xmlsoap.org/soap/
encoding/" namespace="http://www.www.ru/"/>
            </input>
            <output>
                <soap:body use="encoded" encodingStyle="http://schemas.xmlsoap.org/soap/
encoding/" namespace="http://www.www.ru/"/>
            </output>
        </operation>
        <operation name="secondMethod">
            <soap:operation soapAction="http://www.www.ru/#secondMethod"/>
            <input>
                <soap:body use="encoded" encodingStyle="http://schemas.xmlsoap.org/soap/
encoding/" namespace="http://www.www.ru/"/>
            </input>
            <output>
                <soap:body use="encoded" encodingStyle="http://schemas.xmlsoap.org/soap/
encoding/" namespace="http://www.www.ru/"/>
            </output>
        </operation>
    </binding>
    <service name="testMethodService">
        <port name="testMethodPort" binding="tns:testMethodBinding">
            <soap:address location="http://www.www.ru/"/>
        </port>
    </service>
    <message name="testMethodIn"/>
    <service name="secondMethodService">
        <port name="secondMethodPort" binding="tns:secondMethodBinding">
            <soap:address location="http://www.api.ru/v2/test"/>
        </port>
    </service>
