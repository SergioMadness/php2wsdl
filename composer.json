{
  "name": "professionalweb/php2wsdl",
  "description": "Create WSDL files form PHP classes.",
  "type": "library",
  "keywords": [
    "wsdl",
    "wsdl generator",
    "web services"
  ],
  "version": "0.6",
  "homepage": "https://github.com/SergioMadness/php2wsdl",
  "license": "MIT",
  "authors": [
    {
      "name": "Dragos Protung",
      "email": "dragos@protung.ro"
    },
    {
      "name": "Sergey Zinchenko",
      "email": "zinchenko@web-development.pw"
    }
  ],
  "require": {
    "php": ">=5.4",
    "wingu/reflection": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "satooshi/php-coveralls": "*",
    "codeclimate/php-test-reporter": "*"
  },
  "autoload": {
    "psr-4": {
      "PHP2WSDL\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PHP2WSDL\\Test\\": "tests"
    }
  }
}
